                android:layout_toRightOf="@id/carasco"
                android:id="@+id/dimariame"
                android:textColor="#000000"
                android:layout_marginTop="6dp">

            </TextView>

            <ImageView
                android:layout_width="24dp"
                android:layout_height="30dp"
                android:layout_marginRight="20dp"
