<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".profileFragment">


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="54dp"
        android:id="@+id/mbappe"
        android:background="@color/giddy">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/yes"
            >



            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Account"
                android:textSize="22sp"
                android:layout_marginLeft="30dp"
                android:textStyle="bold"
                android:textColor="#ffffff"
                android:layout_marginTop="8dp"
                >

            </TextView>






        </RelativeLayout>

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/mbappe"
        android:background="#F3F3F3"

        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="MY SWAYY ACCOUNT"
            android:textSize="14sp"
            android:id="@+id/acc"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="14dp"
            >

        </TextView>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="216dp"
            android:layout_below="@id/acc"
            android:layout_marginTop="10dp"
            android:background="#ffffff"
            android:id="@+id/leeds"
            android:layout_marginRight="10dp"
            android:layout_marginLeft="10dp">



            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:id="@+id/march"

                android:layout_marginTop="20dp">

                <ImageView
                    android:layout_width="24dp"
                    android:layout_height="30dp"
                    android:layout_marginLeft="10dp"
                    android:tint="#000000"
                    android:id="@+id/auba"
                    android:src="@drawable/ic_baseline_favorite_border_24">

                </ImageView>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:text="Manage my Account"
                    android:layout_toRightOf="@id/auba"
                    android:id="@+id/smith"
                    android:textColor="#000000"
                    android:layout_marginTop="6dp">

                </TextView>

                <ImageView
                    android:layout_width="24dp"
                    android:layout_height="30dp"
                    android:layout_marginRight="20dp"
                    android:tint="#000000"
                    android:layout_alignParentRight="true"
                    android:id="@+id/neymarbarca"

                    android:src="@drawable/ic_baseline_keyboard_arrow_right_24">

                </ImageView>

            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:id="@+id/april"
                android:layout_below="@id/march"
                android:layout_marginTop="20dp">

                <ImageView
                    android:layout_width="24dp"
                    android:layout_height="30dp"
                    android:layout_marginLeft="10dp"
                    android:tint="#000000"
                    android:id="@+id/carasco"
                    android:src="@drawable/ic_baseline_youtube_searched_for_24">

                </ImageView>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:text="Chat settings"
                    android:layout_toRightOf="@id/carasco"
                    android:id="@+id/dimaria"
                    android:textColor="#000000"
                    android:layout_marginTop="6dp">

                </TextView>

                <ImageView
                    android:layout_width="24dp"
                    android:layout_height="30dp"
                    android:layout_marginRight="20dp"
                    android:tint="#000000"
                    android:layout_alignParentRight="true"
                    android:id="@+id/neymarpsg"

                    android:src="@drawable/ic_baseline_keyboard_arrow_right_24">

                </ImageView>

            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:id="@+id/may"
                android:layout_below="@id/april"
                android:layout_marginTop="20dp">

                <ImageView
                    android:layout_width="24dp"
                    android:layout_height="30dp"
                    android:layout_marginLeft="10dp"
                    android:tint="#000000"
                    android:id="@+id/pogba"
                    android:src="@drawable/ic_notifications">

                </ImageView>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:text="Notifications"
                    android:layout_toRightOf="@id/pogba"
                    android:id="@+id/bruno"
                    android:textColor="#000000"
                    android:layout_marginTop="6dp">

                </TextView>

                <ImageView
                    android:layout_width="24dp"
                    android:layout_height="30dp"
                    android:layout_marginRight="20dp"
                    android:tint="#000000"
                    android:layout_alignParentRight="true"
                    android:id="@+id/neymarmanu"

                    android:src="@drawable/ic_baseline_keyboard_arrow_right_24">

                </ImageView>

            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:id="@+id/june"
                android:layout_below="@id/may"
                android:layout_marginTop="20dp">

                <ImageView
                    android:layout_width="24dp"
                    android:layout_height="30dp"
                    android:layout_marginLeft="10dp"
                    android:tint="#000000"
                    android:id="@+id/willian"
                    android:src="@drawable/ic_baseline_feedback_24">

                </ImageView>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:text="Feedback"
                    android:layout_toRightOf="@id/willian"
                    android:id="@+id/saka"
                    android:textColor="#000000"
                    android:layout_marginTop="6dp">

                </TextView>

                <ImageView
                    android:layout_width="24dp"
                    android:layout_height="30dp"
                    android:layout_marginRight="20dp"
                    android:tint="#000000"
                    android:layout_alignParentRight="true"
                    android:id="@+id/neymarchelsea"

                    android:src="@drawable/ic_baseline_keyboard_arrow_right_24">

                </ImageView>

            </RelativeLayout>

        </RelativeLayout>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="MY SETTINGS"
            android:textSize="14sp"
            android:id="@+id/accna"
            android:layout_below="@id/leeds"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="14dp"
            >

        </TextView>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="110dp"
            android:layout_below="@id/accna"
            android:layout_marginRight="10dp"
            android:layout_marginLeft="10dp"
            android:background="#ffffff"
            android:id="@+id/marq"
            android:layout_marginTop="10dp">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="12dp"
                android:id="@+id/ndakugunda">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:text="Details"

                    android:id="@+id/ronaldofree"
                    android:textColor="#000000"
                    android:layout_marginTop="6dp">

                </TextView>

                <ImageView
                    android:layout_width="24dp"
                    android:layout_height="30dp"
                    android:layout_marginRight="20dp"
                    android:tint="#000000"
                    android:layout_alignParentRight="true"
                    android:id="@+id/messibarcafree"

                    android:src="@drawable/ic_baseline_keyboard_arrow_right_24">

                </ImageView>

            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="12dp"
                android:layout_below="@+id/ndakugunda"
                android:id="@+id/ndakugundasong">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:text="Settings"

                    android:id="@+id/ronaldofreeme"
                    android:textColor="#000000"
                    android:layout_marginTop="6dp">

                </TextView>

                <ImageView
                    android:layout_width="24dp"
                    android:layout_height="30dp"
                    android:layout_marginRight="20dp"
                    android:tint="#000000"
                    android:layout_alignParentRight="true"
                    android:id="@+id/messibarcafreeme"

                    android:src="@drawable/ic_baseline_keyboard_arrow_right_24">

                </ImageView>

            </RelativeLayout>

        </RelativeLayout>
        <com.google.android.material.button.MaterialButton
            style="@style/AppTheme.Button.OutlinedAccent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="LOGOUT"
            android:layout_centerHorizontal="true"
            android:textSize="16sp"
            android:id="@+id/accnad"
            android:layout_below="@id/marq"
            android:layout_marginTop="20dp"
            android:layout_marginLeft="14dp"
            >

        </com.google.android.material.button.MaterialButton>

    </RelativeLayout>


    </RelativeLayout>